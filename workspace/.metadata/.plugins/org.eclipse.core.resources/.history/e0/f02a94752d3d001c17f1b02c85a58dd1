package days14;
// 객체 배열
class Student2{
	int bun;
	String name;
	int [] score;
	static int count=0;
	Student2(){
		score=new int [3];
		bun=++count;
		name="std-"+bun;
	}
	Student2(int a, int b, int c){
		this();
		score[0] = a;
		score[1] = b;
		score[2] = c;
	}
}
public class Class27 {

	public static void main(String[] args) {
//		int [] a = new int [5];
//		String [] s = new String[5];
		Student2 [] std = new Student2[5];
		std[0].bun=1;
	}

}
