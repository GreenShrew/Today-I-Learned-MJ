package days02;

public class JavaThird {

	public static void main(String[] args) {
		System.out.printf("\\n : new line");
		System.out.printf("\\t : tab");
		System.out.printf("\\b : 백스페이스");
		System.out.printf("\\' : ' 출력");
		System.out.printf("\\\" : \" 출력");
		System.out.printf("%% : % 출력");
		System.out.printf("\\\\ : 역슬래시 출력");
	}

}
