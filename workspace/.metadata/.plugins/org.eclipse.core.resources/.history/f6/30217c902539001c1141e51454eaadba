package simpleProblem;

import java.util.Scanner;

public class dustmqwkd {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		for(int i=2;i<=9;i++) {
			for(int j=1;j<=9;j++) {
				System.out.printf("%d x %d = %d\t",i,j,i*j);
			}
			System.out.println();
		}
	}

}
