package simpleProblem;

import java.util.Scanner;

public class dustmqwkd {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		for(int i=1;i<11;i++) {
			for(int j=1;j<11;j++) {
				if(j<i) {
					System.out.print("  ");
				}else {
					System.out.print("#");
				}
			}
			System.out.println();
		}
		
		
	}

}
