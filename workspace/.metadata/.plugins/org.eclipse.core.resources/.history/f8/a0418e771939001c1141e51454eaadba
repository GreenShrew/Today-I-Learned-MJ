		System.out.println("몇년?");
		int y = Integer.parseInt(sc.nextLine());
		System.out.println("몇월?");
		int m = Integer.parseInt(sc.nextLine());
		System.out.println("몇일?");
		int day = Integer.parseInt(sc.nextLine());
		int total=0;
		
		for(int i=1;i<y;i++) {
			if(y%4==0 && y!=100 || y%400==0) {
				total+=366;
			}
		}
	}

}
