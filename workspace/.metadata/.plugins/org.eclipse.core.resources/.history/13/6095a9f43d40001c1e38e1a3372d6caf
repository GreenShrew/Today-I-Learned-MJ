package days16;
class SuperG{}
class SubG1 extends SuperG{}
class SubG2 extends SuperG{}
class SubG3 extends SuperG{}	// 부모 클래스를 상속받은 3개의 자식 클래스
public class Extends08_TypeCasting03 {

	public static void main(String[] args) {
		SuperG s = new SuperG();
		SuperG s1 = new SubG1();
		SuperG s2 = new SubG2();
		SuperG s3 = new SubG3();
	}

}
