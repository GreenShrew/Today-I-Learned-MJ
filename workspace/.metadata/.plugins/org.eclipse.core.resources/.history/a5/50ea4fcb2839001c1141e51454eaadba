package simpleProblem;

import java.util.Scanner;

public class dustmqwkd {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		for(int i=1; i>=10; i++) {
			for(int j=1; j<=i+9 ; j++) {
				if(j>=11-i) {
					System.out.printf("#");
				}else {
					System.out.printf("  ");
				}
			}
			System.out.println();
		}
		
		
	}

}
