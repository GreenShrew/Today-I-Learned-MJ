package days02;

public class JavaThird {

	public static void main(String[] args) {
		System.out.printf("\\n : new line\n");
		System.out.printf("\\t : tab\n");
		System.out.printf("\\b : 백스페이스\n");
		System.out.printf("\\' : ' 출력\n");
		System.out.printf("\\\" : \" 출력\n");
		System.out.printf("%%%% : %% 출력\n");
		System.out.printf("\\\\ : 역슬래시 출력\n");
	}

}
