package days14;
class Student2{
	int bun;
	String name;
	int [] score;
	static int count=0;
	Student2(){
		score=new int [3];
		bun=++count;
		name="std-"+bun;
	}
	Student2(int a, int b, int c){
		this();
		score[0] = a;
		score[1] = b;
		score[2] = c;
	}
}
