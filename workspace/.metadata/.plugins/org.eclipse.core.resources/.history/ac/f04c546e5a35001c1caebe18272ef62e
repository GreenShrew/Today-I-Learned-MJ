package days06;

import java.util.Scanner;

public class ControlOpFor06 {

	public static void main(String[] args) {
		// 내가 입력한 정수값의 팩토리얼(n!) 을 구해보자.
		// 6! = 1*2*3*4*5*6
		Scanner sc = new Scanner(System.in);
		int i,k;
		int fact=1;
		
		System.out.printf("출력할 팩토리얼을 입력하세요 : ");
		k = sc.nextInt();
		for(i=1;i<=k;i++) {
			fact = fact*i;
		}
		System.out.println(fact);
	}

}
